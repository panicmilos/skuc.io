package skuc.io;

import skuc.io.skuciocore.models.events.kjar.AggregateParam;
import skuc.io.skuciocore.models.events.device.ValueReceived;

rule "Require Aggregation"
  timer (cron: 0/5 * * * * ?)
  when
    ValueReceived($paramName: paramName)
    not AggregateParam(paramName == $paramName)
  then
    insert(new AggregateParam($paramName));
end;